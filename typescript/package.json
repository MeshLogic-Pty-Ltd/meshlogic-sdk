{
  "name": "@meshlogic/sdk",
  "version": "0.1.0",
  "description": "Official TypeScript/JavaScript SDK for the MeshLogic API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MeshLogic-Pty-Ltd/meshlogic-sdk.git",
    "directory": "typescript"
  },
  "keywords": [
    "meshlogic",
    "security",
    "monitoring",
    "ebpf",
    "endpoint",
    "api",
    "sdk"
  ],
  "author": "Mesh Logic Pty Ltd <support@meshlogic.ai>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/MeshLogic-Pty-Ltd/meshlogic-sdk/issues"
  },
  "homepage": "https://meshlogic.ai/docs/sdk",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
